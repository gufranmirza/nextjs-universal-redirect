  "description": "Higher order component to handle both client and server side redirecting with Next.js",
  "main": "dist.js",
  "repository": "https://github.com/gufranmirza/nextjs-universal-redirect",
  "author": "Gufran Mirza",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "next"
  ],
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.23.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.22.0"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "scripts": {
